{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Attach to packager",
        "type": "reactnative",
        "request": "attach",
        "port": 8081,
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Debug Android",
        "type": "reactnative",
        "request": "launch",
        "program": "${workspaceFolder}/.vscode/launchReactNative.js",
        "platform": "android",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
      },
      {
        "name": "Debug iOS",
        "type": "reactnative",
        "request": "launch",
        "program": "${workspaceFolder}/.vscode/launchReactNative.js",
        "platform": "ios",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
      }
    ]
  }
  